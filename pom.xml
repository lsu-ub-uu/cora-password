<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>se.uu.ub.cora</groupId>
		<artifactId>cora-parent</artifactId>
		<version>2.7.0</version>
	</parent>

	<artifactId>password</artifactId>
	<version>1.9.0-SNAPSHOT</version>
	<name>Cora password</name>

	<properties>
		<spring-security-crypto.version>6.4.4</spring-security-crypto.version>
		<bcpkix-jdk18on.version>1.80</bcpkix-jdk18on.version>
		<commons-logging.verison>1.3.5</commons-logging.verison>
		<testutils.version>1.14.0</testutils.version>
		<testspies.version>1.2.0</testspies.version>
	</properties>

	<scm>
		<connection>scm:git:https://github.com/lsu-ub-uu/cora-password.git</connection>
		<developerConnection>scm:git:https://github.com/lsu-ub-uu/cora-password.git</developerConnection>
		<url>https://github.com/lsu-ub-uu/cora-password.git</url>
		<tag>release/1.9</tag>
	</scm>

	<dependencies>
		<dependency>
			<groupId>org.springframework.security</groupId>
			<artifactId>spring-security-crypto</artifactId>
			<version>${spring-security-crypto.version}</version>
		</dependency>
		<dependency>
			<groupId>org.bouncycastle</groupId>
			<artifactId>bcpkix-jdk18on</artifactId>
			<version>${bcpkix-jdk18on.version}</version>
		</dependency>
		<dependency>
			<groupId>commons-logging</groupId>
			<artifactId>commons-logging</artifactId>
			<version>${commons-logging.verison}</version>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>se.uu.ub.cora</groupId>
			<artifactId>testutils</artifactId>
			<version>${testutils.version}</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>se.uu.ub.cora</groupId>
			<artifactId>testspies</artifactId>
			<version>${testspies.version}</version>
			<scope>test</scope>
		</dependency>
	</dependencies>
</project>